# reactive function whihch create a volcano plot
output$plot.volcano <- renderPlot({
req(ranked())
t<-ranked()
EnhancedVolcano(t,
title = paste0("Fold changes for this contrast group: ", input$top_coef),
subtitle = "",
lab = rownames(t),
x = 'logFC',
y = 'P.Value',
xlab = "Fold Change",
ylab = "Significance",
legendPosition = 'right',
transcriptLabSize = 4)
})
